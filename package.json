{
  "name": "@digitalroute/plutt",
  "version": "1.0.0",
  "author": "Julius Celik",
  "bin": {
    "plutt": "./bin/run"
  },
  "bugs": "https://github.com/digitalroute/plutt/issues",
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "fs-extra": "^8.1.0",
    "prettier": "^1.19.1",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "tslib": "^1.10.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@semantic-release/git": "^8.0.0",
    "@types/chai": "^4.2.7",
    "@types/fs-extra": "^8.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.13",
    "@types/url-join": "^4.0.0",
    "@types/webpack": "^4.41.2",
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "concurrently": "^5.0.2",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.9.0",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "semantic-release": "^16.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/templates"
  ],
  "homepage": "https://github.com/digitalroute/plutt",
  "keywords": [
    "oclif"
  ],
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "plutt",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:digitalroute/plutt.git"
  },
  "scripts": {
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "prettier": "prettier --write --list-different \"**/*.{js,ts,json,md,yml}\"",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "update-docs": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently \"lint-staged\" \"npm run update-docs\""
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
