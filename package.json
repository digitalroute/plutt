{
  "name": "@digitalroute/plutt",
  "version": "0.0.0-semantically-released",
  "author": "Julius Celik",
  "bin": {
    "plutt": "./bin/run"
  },
  "bugs": "https://github.com/digitalroute/plutt/issues",
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@types/babel__core": "^7.1.3",
    "@types/mkdirp": "^1.0.0",
    "@types/react-dev-utils": "^9.0.3",
    "@types/semver": "^7.1.0",
    "@vue/cli-service": "^4.3.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-plugin-transform-commonjs-es2015-modules": "^4.0.1",
    "babel-preset-react-app": "^9.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chokidar": "^3.3.1",
    "cosmiconfig": "^6.0.0",
    "css-loader": "^3.4.2",
    "csstype": "^2.6.8",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "file-loader": "^5.0.2",
    "fs-extra": "^8.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^1.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "prettier": "^1.19.1",
    "react-dev-utils": "^10.1.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "semver": "^7.2.1",
    "terser-webpack-plugin": "^2.3.4",
    "ts-loader": "^6.2.1",
    "tslib": "^1.10.0",
    "url-join": "^4.0.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@rollup/plugin-json": "^4.0.2",
    "@semantic-release/git": "^8.0.0",
    "@types/chai": "^4.2.7",
    "@types/fs-extra": "^8.0.1",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.13",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/url-join": "^4.0.0",
    "@types/webpack": "^4.41.2",
    "@types/webpack-manifest-plugin": "^2.1.0",
    "@types/workbox-webpack-plugin": "^4.1.0",
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.9.0",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "semantic-release": "^17.0.4",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/templates"
  ],
  "homepage": "https://github.com/digitalroute/plutt",
  "keywords": [
    "oclif"
  ],
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "plutt",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:digitalroute/plutt.git"
  },
  "scripts": {
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "prettier": "prettier --write --list-different \"**/*.{js,ts,json,md,yml}\"",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "update-docs": "oclif-dev readme && git add README.md",
    "lint-staged": "lint-staged"
  },
  "types": "lib/index.d.ts",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all --serial lint-staged update-docs"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  }
}
